{
  "name": "@kilfro/tags-input",
  "version": "1.0.4",
  "description": "Component for managing tags",
  "keywords": ["react", "input", "tags", "tag"],
  "author": "Kirill Frolov",
  "license": "MIT",
  "repository": "kilfro/tags-input",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "babel-cli": "^6.26.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "gh-pages": "^2.2.0",
    "jest": "26.4.2",
    "jsdom": "^16.4.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.css": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/index.js",
      "!**/setupTests.js",
      "!**/App.jsx"
    ]
  }
}
